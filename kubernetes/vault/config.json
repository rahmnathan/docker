{
  "backend": {
    "postgresql": {
      "connection_url": "postgres://vault:<password>@<IP Address>:5432/vault?sslmode=disable"
    }
  },
  "listener": {
    "tcp": {
      "address": "0.0.0.0:8200",
      "tls_disable": false,
      "tls_cert_file": "/vault/config/cert.pem",
      "tls_key_file": "/vault/config/key.pem"
    }
  },
  "default_lease_ttl": "168h",
  "max_lease_ttl": "0h",
  "ui": true
}
